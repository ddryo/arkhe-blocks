        "gulp-sass-glob": "^1.1.0",
        "gulp-uglify": "^3.0.2",
        "hex-to-rgba": "^2.0.1",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "npm-check-updates": "^3.2.2",
        "npm-run-all": "^4.1.5",
        "po2json": "^1.0.0-beta-2",
