        "@fortawesome/react-fontawesome": "^0.1.11",
        "@wordpress/icons": "^2.8.0",
        "@wordpress/scripts": "^12.5.0",
        "classnames": "^2.2.6",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
