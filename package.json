{
    "name": "test",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "postinstall": "composer install && composer set-wpcs",
        "phpcs": "./vendor/bin/phpcs",
        "build:js": "npx gulp minifyJs",
        "build:css": "npx gulp compileScss",
        "build:blocks": "wp-scripts build",
        "build": "npm run build:js && npm run build:blocks && npm run build:css",
        "watch:js": "nodemon --watch src/js -e js -x \"npm run build:js\"",
        "watchs:css": "nodemon --watch src -e scss -x \"npm run build:css\"",
        "watch:blocks": "nodemon --watch src/blocks -e js -x \"npm run build:blocks\"",
        "watch:lang": "nodemon --watch languages -e po -x \"npm run po2json\"",
        "watch": "npx npm-run-all --parallel watchs:css watch:js watch:blocks",
        "po2json": "po2json languages/arkhe-blocks-ja.po languages/arkhe-blocks-ja-arkhe-blocks-lang.json -f jed"
    },
    "author": "",
    "license": "ISC",
