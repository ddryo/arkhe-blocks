{
    "apiVersion": 2,
    "name": "arkhe-blocks/rss",
    "title": "RSS list",
    "category": "arkhe-blocks",
    "keywords": [
        "arkhe",
        "rss"
    ],
    "supports": {
        "anchor": true,
        "className": false
    },
    "attributes": {
        "className": {
            "type": "string",
            "default": ""
        },
        "rssUrl": {
            "type": "string",
            "default": ""
        },
        "pageName": {
            "type": "string",
            "default": ""
        },
        "listType": {
            "type": "string",
            "default": "card"
        },
        "listCountPC": {
            "type": "number",
            "default": 3
        },
        "listCountSP": {
            "type": "number",
            "default": 3
        },
        "showSite": {
            "type": "boolean",
            "default": true
        },
        "showDate": {
            "type": "boolean",
            "default": true
        },
        "showAuthor": {
            "type": "boolean",
            "default": false
        },
        "showThumb": {
            "type": "boolean",
            "default": true
        },
        "useCache": {
            "type": "boolean",
            "default": true
        },
        "hTag": {
            "type": "string",
            "default": "div"
        }
    },
    "editorScript": "file:../../../dist/blocks/rss/index.js",
    "textdomain": "arkhe-blocks"
}